#include <iostream>
#include <cstdio>
using namespace std;

int returner(int a, int b, int c, int d)
{
    int max, max1, max2;
    
    if (a>b)
    {
        max1=a;
    }
    else if(a<b)
    {
        max1=b;
    }
    
    if (c>d)
    {
        max2=c;
    }
    else if(c<d)
    {
        max2=d;
    }
    
    if (max1>max2)
    {
        max=max1;
    }
    else if (max1<max2)
    {
        max=max2;
    }
    
    return max;
    
    
}

int main() {
    int a, b, c, d;
   cin>>a>>b>>c>>d;
    
    cout<<returner(a,b,c,d);
    return 0;
}
