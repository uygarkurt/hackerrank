bool primeChecker(int k)
{
	if(k==1)
		return false;
	for(int i=2; i<=k/2; i++)
	{
		if(k%i==0)
			return false;
	}
	//if it is prime
	return true;
}

void simplify(int m)
{
	for(int i=0; i<m; i++)
		for(int j=0; j<m; j++)
		{
			if(primeChecker(tree[i][j]))
				tree[i][j]==0;
		}
}

int calculate(int m)
{
	for(int x=m-2; x>=0; x--)
	{
		for(int y=0; y<=x; y++)
		{
			tree[x][y]+=max(tree[x+1][y], tree[x+1][y+1]);
		}
	}
	return tree[0][0];
}

int main()
{
	simplify(4);	
	printf("%d\n", calculate(4));
	return 0;
}
